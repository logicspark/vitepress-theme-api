#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/palm/SandBox/vitepress-theme-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/palm/SandBox/vitepress-theme-api/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-node@0.30.1_@types+node@18.17.11/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../vite-node@0.30.1_@types+node@18.17.11/node_modules/vite-node/vite-node.mjs" "$@"
fi
